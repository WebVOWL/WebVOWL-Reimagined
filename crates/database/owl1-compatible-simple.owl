<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.com/owl1#"
    xml:base="http://www.example.com/owl1"
    xmlns:o="http://www.example.com/owl1#"
    xmlns:obo="http://purl.obolibrary.org/obo/"
    xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xml="http://www.w3.org/XML/1998/namespace"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://www.example.com/owl1">
        <owl:versionIRI rdf:resource="http://www.example.com/owl1/v1"/>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



    <!-- http://www.example.com/owl1#NonEmptyString -->

    <rdfs:Datatype rdf:about="http://www.example.com/owl1#NonEmptyString"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->




    <!-- http://www.example.com/owl1#hasChild -->

    <owl:ObjectProperty rdf:about="http://www.example.com/owl1#hasChild">
        <rdfs:domain rdf:resource="http://www.example.com/owl1#Person"/>
        <rdfs:range rdf:resource="http://www.example.com/owl1#Person"/>
    </owl:ObjectProperty>



    <!-- http://www.example.com/owl1#hasParent -->

    <owl:ObjectProperty rdf:about="http://www.example.com/owl1#hasParent">
        <rdfs:domain rdf:resource="http://www.example.com/owl1#Person"/>
        <rdfs:range rdf:resource="http://www.example.com/owl1#Person"/>
        <owl:inverseOf rdf:resource="http://www.example.com/owl1#hasChild"/>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->




    <!-- http://www.example.com/owl1#Person -->

    <owl:Class rdf:about="http://www.example.com/owl1#Person"/>



    <!-- http://www.example.com/owl1#Parent -->

    <owl:Class rdf:about="http://www.example.com/owl1#Parent">
        <rdfs:subClassOf rdf:resource="http://www.example.com/owl1#Person"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.com/owl1#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.com/owl1#hasChild"/>
                        <owl:someValuesFrom rdf:resource="http://www.example.com/owl1#Person"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

    <!-- http://www.example.com/owl1#ChildlessPerson -->

    <owl:Class rdf:about="http://www.example.com/owl1#ChildlessPerson">
        <rdfs:subClassOf rdf:resource="http://www.example.com/owl1#Person"/>
        <owl:disjointWith rdf:resource="http://www.example.com/owl1#Parent"/>
    </owl:Class>

    <!-- http://www.example.com/owl1#Mother -->

    <owl:Class rdf:about="http://www.example.com/owl1#Mother">
        <rdfs:subClassOf rdf:resource="http://www.example.com/owl1#Parent"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.com/owl1#Woman"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.com/owl1#hasChild"/>
                        <owl:someValuesFrom rdf:resource="http://www.example.com/owl1#Person"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

    <!-- http://www.example.com/owl1#OnlyChild -->

    <owl:Class rdf:about="http://www.example.com/owl1#OnlyChild">
        <rdfs:subClassOf rdf:resource="http://www.example.com/owl1#Person"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.com/owl1#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.com/owl1#hasParent"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.example.com/owl1#Parent"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.example.com/owl1#hasChild"/>
                                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>


</rdf:RDF>